library(causaleffect)
library(bnlearn)
library(igraph)
###########################################################
#create asia network for causaleffect package
bn.fml <- graph.formula(asia-+tub,
smoke-+lung,
smoke-+bronc,
tub-+either,
lung-+either,
either-+xray,
either-+dysp,
bronc-+dysp)
###########################################################
#load asia network
load("~/Dropbox/inaoe/ACE-IntervComp/Data/bnlearn-data/asia.rda")
#sample data from bnlearn asia network
asia.data <- rbn(bn,n = 10000)
#create an empty dag asia model
asia.dagT <- model2network("[asia][smoke][tub|asia][lung|smoke][either|tub:lung][bronc|smoke][xray|either][dysp|either:bronc]")
#obtaining the cpdag
asia.cpdag <- cpdag(asia.dagT,moral = FALSE)
## Extending CPDAG
#Extending the cpdag to DAG1
asia.dag1 <- set.arc(asia.cpdag,from = "tub",to = "asia")
asia.dag1 <- set.arc(asia.dag1,from = "lung",to = "smoke")
asia.dag1 <- set.arc(asia.dag1,from = "smoke",to = "bronc")
asia.dag1.fml <- igraph.from.graphNEL(as.graphNEL(asia.dag1))
#Extending the cpdag to DAG2
asia.dag2 <- set.arc(asia.cpdag,from = "asia",to = "tub")
asia.dag2 <- set.arc(asia.dag2,from = "lung",to = "smoke")
asia.dag2 <- set.arc(asia.dag2,from = "smoke",to = "bronc")
asia.dag2.fml <- igraph.from.graphNEL(as.graphNEL(asia.dag2))
#Extending the cpdag to DAG3
asia.dag3 <- set.arc(asia.cpdag,from = "tub",to = "asia")
asia.dag3 <- set.arc(asia.dag3,from = "smoke",to = "lung")
asia.dag3 <- set.arc(asia.dag3,from = "bronc",to = "smoke")
asia.dag3.fml <- igraph.from.graphNEL(as.graphNEL(asia.dag3))
#Extending the cpdag to DAG4
asia.dag4 <- set.arc(asia.cpdag,from = "asia",to = "tub")
asia.dag4 <- set.arc(asia.dag4,from = "smoke",to = "lung")
asia.dag4 <- set.arc(asia.dag4,from = "bronc",to = "smoke")
asia.dag4.fml <- igraph.from.graphNEL(as.graphNEL(asia.dag4))
#Extending the cpdag to DAG5
asia.dag5 <- set.arc(asia.cpdag,from = "tub",to = "asia")
asia.dag5 <- set.arc(asia.dag5,from = "smoke",to = "lung")
asia.dag5 <- set.arc(asia.dag5,from = "smoke",to = "bronc")
asia.dag5.fml <- igraph.from.graphNEL(as.graphNEL(asia.dag5))
#Extending the cpdag to DAG6
asia.dag6 <- set.arc(asia.cpdag,from = "asia",to = "tub")
asia.dag6 <- set.arc(asia.dag6,from = "smoke",to = "lung")
asia.dag6 <- set.arc(asia.dag6,from = "smoke",to = "bronc")
asia.dag6.fml <- igraph.from.graphNEL(as.graphNEL(asia.dag6))
##learn parameters (CPTs)
asia.par1 <- bn.fit(asia.dag1,data = asia.data,method = "mle")
asia.par2 <- bn.fit(asia.dag2,data = asia.data,method = "mle")
